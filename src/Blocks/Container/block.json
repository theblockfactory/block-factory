{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "block-factory/container",
  "title": "Container",
  "category": "block-factory",
  "icon": "layout",
  "description": "Container Block",
  "keywords": [
    "container",
    "block"
  ],
  "version": "1.0.0",
  "textdomain": "block-factory",
  "attributes": {
    "justifyContent": {
      "type": "string",
      "default": "left"
    },
    "flexWrap": {
      "type": "boolean",
      "default": true
    },
    "padding": {
      "type": "string",
      "default": "10px"
    },
    "paddingUnit": {
      "type": "string",
      "default": "px"
    },
    "meta": {
      "type": "object",
      "default": {}
    }
  }
}
